      letb x := ret_both ((x) : (int32)) in
      ControlFlow_Continue (solve_lift (ret_both (true : 'bool)))
    | _ =>
      ControlFlow_Break (solve_lift (ret_both (false : 'bool)))
    end)) : both L1 I1 'bool.
